#include "Motor.h"
/*************************************************************************************************/
void Motor_Init(Motor_Name* Motor, TIM_HandleTypeDef* Timer, uint32_t Channel)
{
	Motor->Timer = Timer;
	Motor->Channel = Channel;
	Motor->Pin = Pin;
	Motor->PORT = PORT;
	//Start Timer
	HAL_TIM_PWM_Start(Motor->Timer, Motor->Channel);
}
