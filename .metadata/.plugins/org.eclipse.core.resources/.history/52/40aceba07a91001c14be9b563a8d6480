#ifndef INC_MOTOR_H_
#define INC_MOTOR_H_

#include "stm32f1xx_hal.h"
typedef struct
{
	TIM_HandleTypeDef* Timer;
	uint32_t Channel;
	uint16_t Pin;
	GPIO_TypeDef* PORT;
	uint8_t duty;
	uint16_t speed;
}Motor_Name;


#endif /* INC_MOTOR_H_ */
