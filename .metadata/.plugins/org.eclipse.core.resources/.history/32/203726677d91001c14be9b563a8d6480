#include "Motor.h"
/*************************************************************************************************/
void Motor_Init(Motor_Name* Motor, TIM_HandleTypeDef* Timer, uint32_t Channel)
{
	Motor->Timer = Timer;
	Motor->Channel = Channel;
	//Start Timer
	if (HAL_TIM_PWM_Start(Motor->Timer, Motor->Channel) != HAL_OK)
	{
		Error_Handler();
	}
}
